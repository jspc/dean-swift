{
    "manifest_version": 2,

    "name": "Dean Swift",
    "description": "Replace the name 'Dean Swift' with 'Hand Shank'",
    "version": "0.1",

    "browser_action": {
        "default_icon": {
            "19": "img/ds-19.png",
            "38": "img/ds-38.png"
        },
        "default_popup": "html/popup.html",
        "default_title": "Dean Swift"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["js/content.js"]
        }
    ]
}
